  <template name="ngdir" value="(function() {&#10;    'use strict';&#10;    angular&#10;    .module('$APP$')&#10;    .directive('$DIRECTIVENAME$', $DIRECTIVENAME$);&#10;&#10;function $DIRECTIVENAME$() {&#10;    var directive = {&#10;        restrict: '$RESTRICT$',&#10;        templateUrl: '$TEMPLATEURL$/$DIRECTIVENAME$.html',&#10;        replace:$REPLACE$,&#10;        scope: {&#10;            $VAR1$: '$VALUE1$',&#10;&#9;&#9; &#10;        },&#10;        link:  $DIRECTIVENAME$Link,&#10;        controller: $DIRECTIVENAME$Ctrl,&#10;        // note: This would be '$DIRECTIVENAME$Ctrl' (the exported controller name, as string)&#10;        // if referring to a defined controller in its separate file.&#10;        controllerAs: '$ALIASCONTROLLER$',&#10;       // bindToController: true // because the scope is isolated&#10;    };&#10;&#10;    return directive;&#10;&#10;    function  $DIRECTIVENAME$Link(scope, el, attr, ctrl) {&#10;        //link function of directive&#10;        $END$&#10;    }&#10;}&#10;&#10;$DIRECTIVENAME$Ctrl.$inject = ['$scope'$INJECT$];&#10;&#10;function $DIRECTIVENAME$Ctrl($scope) {&#10;    // Injecting $scope just for comparison&#10;    var $ALIASCONTROLLER$ = this;&#10;&#10;    &#10;&#10;}&#10;&#10;})();" description="ng directive" toReformat="true" toShortenFQNames="true">
    <variable name="APP" expression="complete()" defaultValue="" alwaysStopAt="true" />
    <variable name="DIRECTIVENAME" expression="fileNameWithoutExtension()" defaultValue="" alwaysStopAt="true" />
    <variable name="RESTRICT" expression="capitalize(String)" defaultValue="&quot;AE&quot;" alwaysStopAt="true" />
    <variable name="TEMPLATEURL" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="REPLACE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VAR1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VALUE1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="ALIASCONTROLLER" expression="decapitalize(String)" defaultValue="&quot;vm&quot;" alwaysStopAt="true" />
    <variable name="INJECT" expression="enum(...)" defaultValue="" alwaysStopAt="true" />
